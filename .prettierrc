{
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss", "prettier-plugin-css-order"],
	"cssDeclarationSorterOrder": "concentric-css",
	"tailwindAttributes": ["className"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"singleQuote": true,
	"trailingComma": "all",
	"endOfLine": "lf",
	"printWidth": 120,
	"tailwindStylesheet": "./src/app.css"
}
